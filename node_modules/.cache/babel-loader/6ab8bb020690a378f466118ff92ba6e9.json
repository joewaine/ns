onsole.log(result.data)\n        })\n\n\n      console.log(response) \n\n\n\n\n    }\n    \n  },\n  created() {\n    this.getUserDetails();\n    this.getUserProducts();\nthis.showProducts();\n\n\n\n  }\n\n};\n</script>\n\n<style scoped>\n\n\n.fix-top-nav{\n    position: fixed;\n    width: 100%;\n    top: 0;\n    z-index: 100;\n}\n\n\n.section.hero{\n  width: 100%;\n  height: 100vh;\n}\n\n\n\n\n.nav-acc-header{\n  padding-top: 140px;\n}\n\n\n@media only screen and (max-width: 1080px) {\n\n.nav-acc-header{\n  padding-top: 80px;\n}\n\n}\n\n</style>"],"sourceRoot":"src/views"}]}